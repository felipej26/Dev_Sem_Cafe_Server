        <div class="col-lg-12">
          <h1 class="titulo-blog">Blog</h1>
        
          <br>

          <% posts.forEach(function(post) { %>
          <a href="../posts/show?id=<%= post.id %>">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 bloco-noticia">
              <img class="foto-noticia" src="<%= post.imagem %>" alt="<%= post.titulo %>">
            </div>
          </a>
          <% }); %>
        </div>